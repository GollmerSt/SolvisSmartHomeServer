<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.example.org/graficData" xmlns:tns="http://www.example.org/graficData" elementFormDefault="qualified">

    <complexType name="ScreenGrafic">
        <annotation>
        	<documentation>Zu vergleichende Grafik eines Bildschirmbereichs</documentation>
        </annotation>
        <sequence>
    		<element name="data" type="string">
    			<annotation>
    				<documentation>Daten der Grafik in Base64-Darstellung</documentation>
    			</annotation></element>
    	</sequence>
    	<attribute name="id" type="string">
    		<annotation>
    			<documentation>Eindeutige Id Ã¼ber alle Grafiken, wird aus control referenziert</documentation>
    		</annotation></attribute>
    </complexType>

    <complexType name="ScreenGrafics">
        <annotation>
        	<documentation>Menge aller Grafiken</documentation>
        </annotation>
        <sequence>
    		<element name="Grafic" type="tns:ScreenGrafic" maxOccurs="unbounded" minOccurs="0"></element>
    	</sequence>
    </complexType>


    <complexType name="SolvisGrafics">
        <annotation>
        	<documentation>Root</documentation>
        </annotation>
        <sequence>
    		<element name="ScreenGrafics" type="tns:ScreenGrafics"></element>
    	</sequence>
    </complexType>

    <element name="SolvisGrafics" type="tns:SolvisGrafics"></element>
</schema>